{
	"env": {
		"browser": true,
		"es2021": true,
		"commonjs": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:react/jsx-runtime",
		"plugin:prettier/recommended"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"prettier/prettier": [
			"error",
			{},
			{
				"usePrettierrc": true,
				"fileInfoOptions": {}
			}
		]
	}
}
